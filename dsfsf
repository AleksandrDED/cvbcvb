#include <iostream>

 using namespace std;

 class Person {
     public:
     int Height, Weight, Gender, Age;
     string Name, Nationality;

     Person (int Height, int Weight, int Gender, int Age, string Name, string Nationality){
        this->Height=Height;
        this->Weight=Weight;
        this->Gender=Gender;
        this->Age=Age;
        this->Name=Name;
        this->Nationality=Nationality;
     }

     void Aging (int Evil) {
         this->Age += Evil;
     }
     void Transgendere (int Gay) {
         this->Gender = Gay;
     }
     void nonhandshakable (Person* Wemen1) {
         cout <<this->Name<< " shoot " <<Wemen1->Name<<endl;
     }

 };
 int main () {
     Person* Man1 = new Person(185,70,1,28, "Marat", "Gypsy");
     Person* Wemen1 = new Person(130,35,0,15, "Natasha", "Asian");
     Man1->nonhandshakable(Wemen1);
 }
